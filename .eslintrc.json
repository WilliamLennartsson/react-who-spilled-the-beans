{
    "extends": "airbnb",
    "rules": {
        "react/prefer-stateless-function": 0,
        "semi": 0,
        "react/jsx-filename-extension": 0,
        "indent": 0,
        "no-use-before-define": 0,
        "no-console": 0,
        "arrow-parens": 0,
        "react/jsx-indent": 0,
        "react/destructuring-assignment": 0,
        "react/prop-types": 0,
        "react/jsx-indent-props": 0,
        "linebreak-style": 0,
        "no-plusplus": 0,
        "consistent-return": 0
    },
    "globals": {
        "fetch": false
    },
    "env": {
        "es6": true
    }
}